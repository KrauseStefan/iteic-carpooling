<?xml version="1.0" encoding="UTF-8"?>
<manifest android:versionCode="1" android:versionName="1.0"
    package="dk.iha.carpooling" xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-sdk android:minSdkVersion="8" android:targetSdkVersion="8"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name">
        <uses-library android:name="com.google.android.maps" />
        <activity android:name=".CreateRoute"/>
        <activity android:name=".SearchForRoute"/>
        <activity android:name=".SearchResults"/>
        <activity android:name=".SearchResultRouteInfo"/>
        <activity android:name=".UserProfile"/>
        <activity android:name=".MyRoutes"/>
        <activity android:name=".Ruting"/>
        <activity android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Login"/>
        <activity android:name=".NewUser"/>
    </application>
</manifest>
